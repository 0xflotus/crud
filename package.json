{
  "name": "@nestjsx/crud",
  "version": "0.1.6",
  "description": "Nest CRUD for RESTful APIs",
  "scripts": {
    "lint": "tslint --project .",
    "test": "node_modules/.bin/jest --verbose --coverage --config ./jest.json",
    "test:e2e": "node_modules/.bin/jest --verbose --config ./test/jest-e2e.json",
    "test:all": "npm run test && npm run test:e2e",
    "coverage": "node_modules/.bin/jest --verbose --coverage --config ./jest.json --coverageReporters=text-lcov | coveralls",
    "clean": "cd build && rm -rf `ls | grep -v \"LICENSE\\|package.json\\|README.md\"`",
    "build": "npm run clean && node_modules/.bin/tsc"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.1",
    "@nestjs/core": "^5.4.1",
    "@nestjs/testing": "^5.4.1",
    "@nestjs/typeorm": "^5.2.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/supertest": "^2.0.7",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "npm-install-peers": "^1.2.1",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  }
}
